<!-- Fill in options for navbar here -->
<nav class='header align-right'>
  <% if current_user %>
    <%= link_to 'Home', root_path, { :class=>'nav-item' } %>
    <%= link_to 'My Account', user_path(current_user), { :class=>'nav-item' }  %>
    <%= render partial: "sessions/logout_button" %>
  <% else %>
    <%= link_to 'Sign Up', new_user_path, { :class=>'nav-item' }  %>
    <%= link_to 'Log In', login_path, { :class=>'nav-item' }  %>
  <% end %>
</nav>
